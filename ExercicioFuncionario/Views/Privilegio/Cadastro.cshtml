
@{
    ViewBag.Title = "Cadastro";
}

<h4>Cadastro de Privilegio</h4>

<form method="post" action="@Url.Action("Store")">

    <div class="form-group">
        <label for="cadastro-privilegio-nome">Privilegio</label>
        <input type="text" id="cadastro-privilegio-nome" name="privilegio.Nome" class="form-control" />
    </div>

    <button type="submit" class="btn btn-success">Salvar</button>

</form>

